{
  "name": "@seed-design/icon-generate-cli",
  "version": "0.0.0-20230208.1",
  "type": "module",
  "scripts": {
    "prepack": "yarn build",
    "update-icons": "SKIP_PDF=true node -r tsm scripts/fetch-and-build-assets.cjs",
    "generate-pdf": "node scripts/convert-svg-to-pdf.cjs",
    "generate-sprite": "node scripts/generate-svg-sprite.cjs",
    "generate-d-ts": "node scripts/generate-json-to-ts.cjs",
    "build": "yarn build:tsc && yarn build:babel",
    "build:tsc": "tsc --emitDeclarationOnly",
    "build:babel": "babel -d lib src -x .tsx"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "svg",
    "drawable",
    "pdf",
    "lib"
  ],
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-modules": "^0.1.5",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@svgr/core": "^6.0.0",
    "@svgr/plugin-jsx": "^6.0.0",
    "@svgr/plugin-prettier": "^6.0.0",
    "@svgr/plugin-svgo": "^6.0.0",
    "@types/react": "^17.0.37",
    "dotenv": "^10.0.0",
    "got": "^11.8.3",
    "jsdom": "^19.0.0",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "svg2pdf.js": "^2.2.0",
    "svg2vectordrawable": "^2.8.1",
    "svgo": "^2.8.0",
    "tasuku": "^1.0.2",
    "tsm": "^2.2.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "svg-sprite": "^2.0.1",
    "string-dedent": "^3.0.1"
  }
}
